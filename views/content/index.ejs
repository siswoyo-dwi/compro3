<script>
  document.getElementById('menu-toggle').addEventListener('click', function() {
      document.getElementById('mobile-menu').classList.toggle('hidden');
  });
   // Select all buttons
   const buttons = document.querySelectorAll(".filter-btn");

// Add click event to each button
buttons.forEach(button => {
button.addEventListener("click", () => {
  // Remove active styles from all buttons
  buttons.forEach(btn => btn.classList.remove("px-4", "py-1.5", "text-sm", "font-medium", "rounded-full", "shadow", "bg-gray-900", "text-white"));
  buttons.forEach(btn => btn.classList.add("px-3", "py-1.5", "text-sm", "text-gray-600", "hover:text-gray-900", "transition"));

  // Add active styles to clicked button
  button.classList.add("px-4", "py-1.5", "text-sm", "font-medium", "rounded-full", "shadow", "bg-gray-900", "text-white");
  button.classList.remove("px-3", "py-1.5", "text-sm", "text-gray-600", "hover:text-gray-900", "transition");
});
});
</script>
