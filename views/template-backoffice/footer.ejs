<script type="text/javascript">
  function point_format(a) {
    return a.toString().replace(/(\d)(?=(\d{3})+(?!\d))/g, '$1.')
  }
  var rooturl = ''
  let text_delete = {title:'Berhasil',text:'Data berhasil dihapus',icon:'success',type:'success'}
  let text_insert = {title:'Berhasil',text:'Data berhasil ditambahkan',icon:'success', type:'success'}
  let text_edit = {title:'Berhasil',text:'Data berhasil diedit',icon:'success', type:'success'}
  let text_error = {title:'Gagal',text:'Data gagal ditambahkan',icon:'error', type:'error'}
  // Validasi file menggunakan JavaScript
  const MAX_FILE_SIZE = 500 * 1024 ;
function cek_file(file) {
    // Periksa apakah file ada
    const validType = file.type === "image/jpeg";

    if (!file) {
      return "Silakan pilih file terlebih dahulu.";
    }else

    // Validasi ukuran file
    if (file.size > MAX_FILE_SIZE) {
      return "Ukuran file melebihi 500KB.";
    }else

    // Validasi tipe file
    if (!validType) {
      return "Hanya file .jpg yang diperbolehkan."
    }else

    // Reset pesan kesalahan jika lolos validasi
    {return ""}
  }
</script>
